{
  "name": "barber",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"const fs=require('fs');const path=require('path');['.next','node_modules/.cache','node_modules/.prisma'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true});console.log('Deleted:',d)}catch(e){}})\"",
    "dev:clean": "npm run clean && next dev",
    "build:clean": "npm run clean && next build",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "send:appointment-reminders": "tsx scripts/sendAppointmentReminders.ts",
    "mark:appointments-done": "tsx scripts/markPastAppointmentsDone.ts",
    "run:recurring-expenses": "tsx scripts/runRecurringExpenses.ts"
  },
